@model BAP.Web.Models.RegisterViewModel
@{
    ViewBag.Title = "Register";    
}

<!-- MAIN CONTENT SECTION -->
<section class="mainContent clearfix lostPassword">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2 col-xs-12">
                <div class="panel panel-default">
                    <div class="panel-heading"><h3>@ViewBag.Title</h3></div>
                    <div class="panel-body">
                        <p class="help-block">Thank you for registering with us. Please check your email to confirm your account registration.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>